alt - h : yabai -m window --focus west
alt - j : yabai -m window --focus south
alt - k : yabai -m window --focus north
alt - l : yabai -m window --focus east

alt + shift - h : yabai -m window --swap west
alt + shift - j : yabai -m window --swap south
alt + shift - k : yabai -m window --swap north
alt + shift - l : yabai -m window --swap east

alt + ctrl - h : yabai -m window --resize left:10
alt + ctrl - j : yabai -m window --resize bottom:10
alt + ctrl - k : yabai -m window --resize top:10
alt + ctrl - l : yabai -m window --resize right:10

ctrl - 1 : yabai -m display --focus 1
ctrl - 2 : yabai -m display --focus 2
ctrl - 3 : yabai -m display --focus 3
ctrl - 0x1B : yabai -m display prev
ctrl - 0x18 : yabai -m display next

alt - 1 : yabai -m space --focus 1
alt - 2 : yabai -m space --focus 2
alt - 3 : yabai -m space --focus 3
alt - 4 : yabai -m space --focus 4
alt - 5 : yabai -m space --focus 5
alt - 6 : yabai -m space --focus 6
alt - 7 : yabai -m space --focus 7
alt - 8 : yabai -m space --focus 8
alt - 0x1B : yabai -m space --focus prev
alt - 0x18 : yabai -m space --focus next

alt + ctrl - 1 : yabai -m space --display 1
alt + ctrl - 2 : yabai -m space --display 2
alt + ctrl - 3 : yabai -m space --display 3
alt + ctrl - 0x1B : yabai -m space --display prev
alt + ctrl - 0x18 : yabai -m space --display next

alt + shift - 1 : yabai -m window --space 1
alt + shift - 2 : yabai -m window --space 2
alt + shift - 3 : yabai -m window --space 3
alt + shift - 4 : yabai -m window --space 4
alt + shift - 5 : yabai -m window --space 5
alt + shift - 6 : yabai -m window --space 6
alt + shift - 7 : yabai -m window --space 7
alt + shift - 8 : yabai -m window --space 8
alt + shift - 0x1B : yabai -m window --space prev
alt + shift - 0x18 : yaabai -m window --space next

alt + shift - f : yabai -m window --toggle float
alt - f : yabai -m window --toggle fullscreen
